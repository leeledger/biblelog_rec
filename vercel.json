{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Permissions-Policy",
                    "value": "microphone=(*), camera=(*)"
                },
                {
                    "key": "Feature-Policy",
                    "value": "microphone *; camera *"
                }
            ]
        }
    ]
}